{
	"This_is_for_testing": "This_is_for_testing",
	"rootLogFolder": "logs",
	"logFolder": "this_is_replaced_at_run_time: logs/{env}",
	"appLogFolder": "this_is_replaced_at_run_time: logs/{env}/app",
	"accessLogFolder": "this_is_replaced_at_run_time: logs/{env}/access",
	"timeOutShutdown": "10000",
	"gulp": {
		"layoutFiles": [
			"layout.html"
		],
		"frontEndViewsFolder": "frontend/views",
		"frontEndStyleSheets": "frontend/stylesheets/**/*{.sass,.css}",
		"frontEndJavascript": "frontend/js/**/*.js",
		"tmpFolder": ".tmp",
		"tmpFrontEndViewsFolder": ".tmp/frontend/views",
		"tmpVendorFolder": ".tmp/frontend/vendor",
		"tmpStyleSheetFolder": ".tmp/frontend/stylesheets",
		"tmpJavascriptFolder": ".tmp/frontend/js",
		"tmpJavascript": ".tmp/frontend/js/**/*.js",
		"tmpStyleSheets": ".tmp/frontend/stylesheets/**/*.css",
		"testServerFolder": "test/test-server"
	},
	"port": 3000
}
